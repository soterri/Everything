package com.syntax.class11;

public class Large {

	public static void main(String[] args) {

		int[] numbers = { 500, 800, 9000 };

		int largest = numbers[0];

		for (int i = 0; i < numbers.length; i++) {

			if (numbers[i]>largest) {
				largest = numbers[i];
			}
		}
		System.out.println(largest);
	}
}
