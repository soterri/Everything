package com.syntax.class08;

public class StarPattern {
	public static void main(String[] args) {
		
		for (int i=1; i<6; i++) {
			
			for(int j=1; j<=i; j++) {
				
				System.out.print("*");
			}
			System.out.println();
		}
		for(int i=1; i<0=====10; i++) {
			
			for(int j=1; i<=j; j++) {
				System.out.print(j);
			}
			System.out.println();
		}
	}

}
